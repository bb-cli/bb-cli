{
    "name": "semiherdogan/bitbucketcli",
    "description": "Bitbucket Rest Api Cli tool.",
    "keywords": ["bitbucket"],
    "license": "MIT",
    "authors": [
        {
            "name": "Semih ERDOÄžAN",
            "email": "hasansemiherdogan@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.1|^8.0"
    },
    "bin": [
        "bin/bb"
    ],
    "autoload": {
      "files": [
        "src/utils/helpers.php"
      ],
        "psr-4": {
            "BBCli\\BBCli\\": "src/"
        }
    },
    "scripts": {
        "build": "php --define phar.readonly=0 create-phar.php && chmod +x bb.phar"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
